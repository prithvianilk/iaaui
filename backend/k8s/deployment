apiVersion: apps/v1
kind: Deployment
metadata:
  name: {{image_name}}-deployment
spec:
  replicas: {{replicas}}
  selector:
    matchLabels:
      app: {{image_name}}-app
  template:
    metadata:
      labels:
        app: {{image_name}}-app
    spec:
      containers:
        - name: {{image_name}}-container
          image: public.ecr.aws/z2h9g2z1/hack:{{image_name}}